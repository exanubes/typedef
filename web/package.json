{
    "engines": {
        "node": "22.x"
    },
    "name": "@typedef/web",
    "version": "1.0.0",
    "description": "",
    "source": "src/index.html",
    "targets": {
        "prod": {
            "distDir": "../dist/web/",
            "publicUrl": ".",
            "optimize": true
        }
    },
    "scripts": {
        "start": "parcel",
        "build": "parcel build",
        "build:prod": "parcel build --target prod && cp .nojekyll ../dist/web/.nojekyll",
        "lint": "eslint src/**/*.{js,html} && stylelint src/**/*.css",
        "lint:fix": "eslint src/**/*.{js,html} --fix && stylelint src/**/*.css --fix",
        "format": "prettier --write \"src/**/*.{js,css,html}\"",
        "format:check": "prettier --check \"src/**/*.{js,css,html}\"",
        "check": "pnpm run format:check && pnpm run lint"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.11.0",
    "devDependencies": {
        "@html-eslint/eslint-plugin": "^0.52.0",
        "@html-eslint/parser": "^0.52.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-html": "^8.1.3",
        "eslint-plugin-jsdoc": "^61.5.0",
        "eslint-plugin-prettier": "^5.5.4",
        "parcel": "^2.16.3",
        "prettier": "^3.7.4",
        "stylelint": "^16.26.1",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-standard": "^39.0.1",
        "stylelint-prettier": "^5.0.3"
    }
}
